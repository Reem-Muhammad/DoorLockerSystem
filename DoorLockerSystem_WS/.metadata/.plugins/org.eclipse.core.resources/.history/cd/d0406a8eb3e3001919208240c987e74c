#include "LCD.h"
#include "UART.h"

const Uart_ConfigType s_UartConfig = {PARITY_DISABLED, ONE_STOP_BIT, CHAR_8, 9600};
uint8 received_byte = 0;

int main()
{
	LCD_init();
	UART_init(&s_UartConfig);

	while(1)
	{
		received_byte = UART_receiveByte();
		LCD_displayCharacter(received_byte);

	}
}
