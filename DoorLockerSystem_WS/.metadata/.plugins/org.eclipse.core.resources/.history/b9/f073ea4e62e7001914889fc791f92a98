 *      Author: Home
 */


#include "Ocu.h"

const Ocu_ConfigType s_OcuConfig = {OCU_DISABLE,OCU_PRESCALER_1024};


void mainCb()
{
	SET_BIT(PORTC, PC0);
	_delay_ms(500);
	CLEAR_BIT(PORTC, PC0);
	_delay_ms(500);
}


int main()
{
	DDRA = 0xFF;
	DDRB = 0xFF;
	DDRC = 0xFF;
	DDRD = 0xFF;
	SET_BIT(SREG, 7);

	Ocu_setCb(mainCb);

	Ocu_Init(&s_OcuConfig);

	while(1)
	{
		PORTA = TCNT1L;
		PORTB = TCNT1H;
		//PORTD = TIFR;
	}
}
